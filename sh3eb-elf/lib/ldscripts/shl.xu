/* Script for ld -Ur: link w/out relocation, do create constructors */
OUTPUT_FORMAT("coff-shl")
OUTPUT_ARCH(sh)
MEMORY
{
  ram : o = 0x1000, l = 512k
}
SECTIONS
{
  .text :
  {
    *(.text)
    *(.strings)
  }
  .tors :
  {
    ___ctors = . ;
    *(.ctors)
    ___ctors_end = . ;
    ___dtors = . ;
    *(.dtors)
    ___dtors_end = . ;
  } > ram
  .data :
  {
    *(.data)
  }
  .bss :
  {
    *(.bss)
    *(COMMON)
  }
  .stack   :
  {
    *(.stack)
  }
  .stab 0  :
  {
    *(.stab)
  }
  .stabstr 0  :
  {
    *(.stabstr)
  }
}
