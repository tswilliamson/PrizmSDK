/* Script for ld -r: link without relocation */
OUTPUT_FORMAT("coff-sh")
OUTPUT_ARCH(sh)
MEMORY
{
  ram : o = 0x1000, l = 512k
}
SECTIONS
{
  .text :
  {
    *(.text)
    *(.strings)
  }
  .data :
  {
    *(.data)
  }
  .bss :
  {
    *(.bss)
    *(COMMON)
  }
  .stack   :
  {
    *(.stack)
  }
  .stab 0  :
  {
    *(.stab)
  }
  .stabstr 0  :
  {
    *(.stabstr)
  }
}
